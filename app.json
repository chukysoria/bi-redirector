{
  "name": "bi-redirector",
  "scripts": {},
  "env": {
    "AUTH0_CLIENT_ID": {
      "required": "true"
    },
    "AUTH0_DOMAIN": {
      "required": "true"
    },
    "AUTH0_AUDIENCE": {
      "required": "true"
    },
    "AUTH0_CLIENT_SECRET": {
      "required": "true"
    },
    "AUTH0_CALLBACK_URL": {
      "value": "http://localhost:5000/api/authcallback"
    },
    "SECRET_KEY": {
      "generator": "secret"
    }
  },
  "formation": {},
  "addons": [
    "heroku-redis"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/python"
    }
  ]
}
